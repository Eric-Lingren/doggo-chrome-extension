{
  "name": "Doggo Images",
  "manifest_version": 2,
  "version": "0.0.0.1",
  "description": "Loads a random dog image",
  "content_scripts": [{
    "js": [ "content.js"],
    "matches": [ "<all_urls>"]
  }],
  "permissions": ["contextMenus", "activeTab"],
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources" : ["*.html"],
  "icons": {
    "16": "doggo-logo.png",
    "48": "doggo-logo.png",
    "128": "doggo-logo.png"
  }
}